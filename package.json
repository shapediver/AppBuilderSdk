  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "optimize": "tsc && vite optimize --force",
    "preview": "vite preview",
    "eslint": "eslint .",
    "prettier": "prettier --write ./src/ ./*.ts",
    "test": "jest",
    "convert-slug-data": "tsx ./scripts/convertSlugData.ts",
    "build-appbuilder": "scripts/build-appbuilder.sh 0 v1/main",
    "build-appbuilder-platform": "scripts/build-appbuilder.sh 0 app/builder/v1/main",
    "publish-appbuilder": "scripts/build-appbuilder.sh 1 v1/main",
    "publish-appbuilder-platform": "scripts/build-appbuilder.sh 1 app/builder/v1/main",
    "publish": "scripts/build-appbuilder.sh 1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
  	"@feature-sliced/eslint-config": "^0.1.1",
    "@sentry/cli": "^2.58.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.10",
    "@types/post-robot": "^10.0.6",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.3",
    "dotenv": "17.2.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-bundle-analyzer": "^1.3.6",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
