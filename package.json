{
  "name": "shapediver-app-builder",
  "version": "1.4.1",
  "private": true,
  "type": "module",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@iconify/react": "^6.0.0",
    "@mantine/carousel": "^8.1.3",
    "@mantine/charts": "^8.1.3",
    "@mantine/core": "^8.1.3",
    "@mantine/hooks": "^8.1.3",
    "@mantine/notifications": "^8.1.3",
    "@sentry/react": "^8.55.0",
    "@sentry/vite-plugin": "^2.23.0",
    "@shapediver/sdk.geometry-api-sdk-v2": "2.9.1",
    "@shapediver/sdk.platform-api-sdk-v1": "2.28.5",
    "@shapediver/sdk.stargate-sdk-v1": "^1.6.1",
    "@shapediver/viewer.features.attribute-visualization": "3.13.14",
    "@shapediver/viewer.features.drawing-tools": "3.13.14",
    "@shapediver/viewer.features.gumball": "3.13.14",
    "@shapediver/viewer.features.interaction": "3.13.14",
    "@shapediver/viewer.session": "3.13.14",
    "@shapediver/viewer.utils.mime-type": "~1.1.0",
    "@shapediver/viewer.viewport": "3.13.14",
    "gl-matrix": "^3.4.3",
    "immer": "^10.1.1",
    "langfuse": "^3.35.3",
    "openai": "^4.85.4",
    "plausible-tracker": "^0.3.9",
    "post-robot": "^8.0.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-hook": "^5.0.1",
    "react-markdown": "^9.1.0",
    "react-router-dom": "6.26.2",
    "react-scripts": "5.0.1",
    "remark-directive": "^3.0.1",
    "remark-gfm": "^4.0.1",
    "unist-util-visit": "^5.0.0",
    "uuid": "^10.0.0",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "optimize": "tsc && vite optimize --force",
    "preview": "vite preview",
    "eslint": "eslint .",
    "prettier": "prettier --write ./src/ ./*.ts",
    "test": "jest",
    "build-appbuilder": "scripts/build-appbuilder.sh 0 v1/main",
    "build-appbuilder-platform": "scripts/build-appbuilder.sh 0 app/builder/v1/main",
    "publish-appbuilder": "scripts/build-appbuilder.sh 1 v1/main",
    "publish-appbuilder-platform": "scripts/build-appbuilder.sh 1 app/builder/v1/main",
    "publish": "scripts/build-appbuilder.sh 1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/cli": "^2.42.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "@types/post-robot": "^10.0.6",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.18.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}