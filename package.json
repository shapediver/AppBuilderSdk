    "@shapediver/viewer.session": "3.15.2",
    "@shapediver/viewer.shared.types": "3.15.2",
    "@shapediver/viewer.utils.mime-type": "~1.1.0",
    "@shapediver/viewer.viewport": "3.15.2",
    "gl-matrix": "^3.4.4",
    "immer": "^10.2.0",
    "langfuse": "^3.38.6",
